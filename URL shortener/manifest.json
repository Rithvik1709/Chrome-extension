{
  "manifest_version": 3,
  "name": "Basic URL Shortener",
  "version": "1.0",
  "description": "A simple URL shortener that generates short hashes.",
  "permissions": ["clipboardWrite"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
